<resources>

    <!-- Base application theme. -->
    <style name="Netfrixson" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorPrimaryDark">@color/primaryDarkColor</item>
        <item name="colorAccent">@color/secondaryColor</item>
    </style>

    <style name="Netfrixson.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

    </style>

    <style name="Netfrixson.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Netfrixson.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Netfrixson.TextView" parent=""></style>

</resources>
